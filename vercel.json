{
    "version": 2,
    "builds": [
        {
            "src": "Backend/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "AI_backend/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "Fe/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/Backend/index.js"
        },
        {
            "src": "/ai-api/(.*)",
            "dest": "/AI_backend/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "/Fe/$1"
        }
    ]
}